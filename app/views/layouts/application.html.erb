<!DOCTYPE html>
<html>
  <head>
    <title>MulberrySound</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    
    <%= render "shared/application_nav" %>
    
    <% if alert %>
      <p class="alert">
        <%= alert %>
      </p>
    <% elsif notice %>
      <p class="notice">
        <%= notice %>
      </p>
    <% end %>
    
    <%= yield %>
  </body>
</html>
